        <activity android:name="com.example.heartpumping.HPHomeBoard" >
        </activity>
        <activity android:name="com.example.heartpumping.HPHomeFind" >
        </activity>
        <activity android:name="com.example.heartpumping.HPHomeSetting" >
        </activity>
    </application>

</manifest>